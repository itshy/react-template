{
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "globalReturn": true,
      "impliedStrict": true
    }
  },
  "parser": "babel-eslint",
  "rules": {
    "array-bracket-spacing": ["error", "always", { "objectsInArrays": false }],
    "constructor-super": 2,
    "comma-spacing": ["error", { "before": false, "after": true }],
    "curly": 2,
    "eqeqeq": 2,
    "for-direction": 2,
    "getter-return": 2,
    "indent": [2, 2],
    "jsx-quotes": ["error", "prefer-double"],
    "lines-between-class-members": ["error", "always"],
    "no-case-declarations": 2,
    "no-class-assign": 2,
    "no-compare-neg-zero": 2,
    "no-cond-assign": 2,
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty-pattern": 2,
    "no-empty": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-extra-parens": 2,
    "no-fallthrough": 2,
    "no-func-assign": 2,
    "no-global-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": "error",
    "no-multi-spaces": 2,
    "no-new-symbol": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-self-assign": 2,
    "no-sparse-arrays": 2,
    "no-this-before-super": 2,
    "no-undef": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "no-unsafe-negation": 2,
    "no-unused-labels": 2,
    "no-useless-escape": 2,
    "object-curly-spacing": ["error", "always"],
    "require-yield": 2,
    "semi": ["error", "never"],
    "space-before-blocks": "error",
    "use-isnan": 2,
    "valid-typeof": 2,
    "vars-on-top": 2
  },
  "env": {
    "browser": true,
    "es6": true
  }
}